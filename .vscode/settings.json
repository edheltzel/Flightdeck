  "emmet.includeLanguages": {
    "*.njk": "html"
  },
  "files.exclude": {
    "**/.sass-cache/": true,
    "**/_site/": false,
    "**/dist/": false,
    "node_modules/": false
  },
  "search.exclude": {
    "**/.publish/": true,
    "**/.sass-cache/": true,
    "**/css/*": true,
    "*.lock": true,
    ".vscode/**": false,
    "_site/**": true,
    "dist/**": true,
    "node_modules/": true,
    "package-lock.json": true
  }
}
