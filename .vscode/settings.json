{
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "addWords": true,
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:flightdeck}/.vscode/cspell-custom-dictionary-workspace.txt",
      "scope": "workspace"
    }
  },
  "cSpell.words": [
    "allimages",
    "alpinejs",
    "cleancache",
    "Coup√©",
    "divriots",
    "endraw",
    "flightdeck",
    "Heltzel",
    "kleur",
    "Laserwave",
    "outbase",
    "Pico",
    "picocolors",
    "picocss",
    "postbuild",
    "prismjs",
    "spacex",
    "vgscursor",
    "xxxs",
    "xxxxl"
