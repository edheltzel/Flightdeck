{
  "name": "flightdeck-for-eleventy",
  "version": "0.1.0",
  "description": "An opinionated Jamstack starter project for Eleventy.",
  "scripts": {
    "start": "eleventy --serve --config=_flightdeck/.manifest.js",
    "build": "run-s clean 11ty",
    "11ty": "cross-env ENVIRONMENT=prod eleventy --config=_flightdeck/.manifest.js",
    "images": "optimizt src/assets/images",
    "clean": "./.scrub site",
    "purge": "./.scrub purge"
  },
  "author": "Ed Heltzel",
  "license": "WTFPL",
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@funboxteam/optimizt": "^4.0.0",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.8",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.3.0",
    "eleventy-plugin-embed-everything": "^1.14.1",
    "esbuild": "^0.14.54",
    "esbuild-sass-plugin": "^2.3.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.54.3"
  }
}
