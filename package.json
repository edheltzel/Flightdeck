{
  "name": "flightdeck-for-eleventy",
  "version": "0.1.0",
  "description": "An opinionated Jamstack starter project for Eleventy.",
  "scripts": {
    "start": "eleventy --serve --config=_flightdeck/.manifest.js",
    "build": "run-s clean 11ty",
    "11ty": "cross-env ENVIRONMENT=prod eleventy --config=_flightdeck/.manifest.js",
    "images": "optimizt --webp src/assets/images",
    "clean": "./.scrub site",
    "purge": "./.scrub purge"
  },
  "author": "Ed Heltzel",
  "license": "WTFPL",
  "browserslist": "> 0.5%, last 2 versions, Firefox ESR, not dead",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@divriots/jampack": "^0.13.0",
    "alpinejs": "^3.12.3",
    "browserlist": "^1.0.1",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^7.0.3",
    "eleventy-plugin-embed-everything": "^1.16.0",
    "esbuild": "^0.18.18",
    "esbuild-sass-plugin": "^2.10.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.64.2"
  }
}
